<Window x:Class="PerJannersten.UI.WpfPages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:PerJannersten.UI.WpfPages"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Menu Height="25" VerticalAlignment="Top">
        <MenuItem Header="Settings">
            <MenuItem Header="Current Game Settings" Click="OpenCurrentSettings"></MenuItem>
            <MenuItem Header="Current Additional Game Settings" Click="OpenAdditionalCurrentSettings"></MenuItem>
            <MenuItem Header="Default Game Settings" Click="OpenDefaultSettings"></MenuItem>
            <MenuItem Header="Default Additional Game Settings" Click="OpenAdditionalDefaultSettings"></MenuItem>
        </MenuItem>
    </Menu>
</Window>
